  <!--Ash-->
  <RecipeDef ParentName="WarframeBodyPartRecipeBase">
    <defName>Make_Ash_Head</defName>
    <label>make Ash head</label>

    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Gold</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>150</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Gold</li>
        <li>ComponentIndustrial</li>
        <li>Plasteel</li>
        <li>Silver</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <WFPart_Ash_Head>1</WFPart_Ash_Head>
    </products>
  </RecipeDef>
